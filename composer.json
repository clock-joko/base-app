{
    "name": "clock/base",
    "description": "clock-it base source",
    "autoload": {
        "psr-4": {
            "Clock\\Baserepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clock\\Baserepo\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3"
    },
    "repositories": {
        "dev-package": {
            "type": "path",
            "url": "src",
            "options": {
                "symlink": true
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "shin.joko",
            "email": "shin.joko@clock-it.jp"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Clock\\Baserepo\\Providers\\BaseRepositoryProvider",
                "Clock\\Baserepo\\Providers\\CommandServiceProvider"
            ]
        }
    }
}
